{
  "name": "nextless-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "check-types": "tsc --noEmit --pretty",
    "test": "jest",
    "check-backend": "curl --silent http://localhost:4000/health || echo 'Please start Nextless.js backend or something goes wrong in the backend'",
    "cypress": "cypress open --e2e",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test check-backend http://localhost:4000/health dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test check-backend http://localhost:4000/health dev http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.6.13",
    "@floating-ui/react-dom": "^1.0.0",
    "@floating-ui/react-dom-interactions": "^0.10.2",
    "@headlessui/react": "^1.7.3",
    "@hookform/error-message": "^2.0.0",
    "@stripe/stripe-js": "^1.42.0",
    "@types/styled-jsx": "^3.4.4",
    "avvvatars-react": "^0.4.2",
    "aws-amplify": "^4.3.42",
    "classnames": "^2.3.2",
    "nanoid": "^4.0.0",
    "next": "^13.0.0",
    "next-seo": "^5.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.38.0",
    "react-use": "^17.4.0",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.24",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "autoprefixer": "^10.4.13",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "cypress": "^10.11.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^13.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tailwindcss": "^3.6.2",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.2.2",
    "lint-staged": "^13.0.3",
    "msw": "^0.47.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.2.1",
    "typescript": "^4.8.4"
  },
  "license": "ISC"
}
