{
  "name": "nextless-frontend",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "export": "next export",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next out",
    "lint": "next lint",
    "build-types": "tsc --noEmit --pretty",
    "test": "jest",
    "check-backend": "curl --silent http://localhost:4000/health || echo 'Please start Nextless.js backend or something goes wrong in the backend'",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test check-backend http://localhost:4000/health dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test check-backend http://localhost:4000/health dev http://localhost:3000 cypress:headless",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-amplify/auth": "^4.5.9",
    "@headlessui/react": "^1.6.6",
    "@hookform/error-message": "^2.0.0",
    "@stripe/stripe-js": "^1.32.0",
    "@types/styled-jsx": "^3.4.4",
    "avvvatars-react": "^0.4.2",
    "aws-amplify": "^4.3.27",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "next": "^12.2.1",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-use": "^17.4.0",
    "styled-jsx-plugin-postcss": "^4.0.1",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.1",
    "@tailwindcss/forms": "^0.5.2",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.3",
    "@types/react": "^18.0.15",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "cypress": "^10.3.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
